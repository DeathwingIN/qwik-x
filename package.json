{
  "name": "qwik_x",
  "description": "Qweet your feelings",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "migration:generate": "drizzle-kit generate:pg",
    "migration:push": "drizzle-kit push:pg",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.2.6",
    "@builder.io/qwik-city": "^1.2.6",
    "@types/compression": "^1.7.2",
    "@types/crypto-js": "^4.1.1",
    "@types/eslint": "8.40.2",
    "@types/node": "^20.4.0",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "autoprefixer": "^10.4.14",
    "compression": "^1.7.4",
    "daisyui": "^3.5.1",
    "drizzle-kit": "^0.19.12",
    "eslint": "8.44.0",
    "eslint-plugin-qwik": "^1.2.6",
    "pg": "^8.11.3",
    "postcss": "^8.4.23",
    "prettier": "2.8.8",
    "tailwindcss": "^3.3.1",
    "typescript": "5.1.6",
    "undici": "5.22.1",
    "vite": "4.4.0",
    "vite-tsconfig-paths": "4.2.0"
  },
  "dependencies": {
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "jose": "^4.14.4",
    "postgres": "^3.3.5",
    "qwik-spin-delay": "^1.1.0",
    "qwikjs-wrap-balancer": "^0.6.0"
  }
}
